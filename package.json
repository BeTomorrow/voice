{
  "name": "@react-native-voice/voice",
  "description": "React Native Native Voice library for iOS and Android",
  "version": "3.2.2",
  "author": "Sam Wenke <samwenke@gmail.com>",
  "private": false,
  "homepage": "https://github.com/react-native-voice/voice",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@tsconfig/node10": "^1.0.7",
    "@types/invariant": "^2.2.31",
    "@types/react-native": "0.61.17",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@typescript-eslint/typescript-estree": "^5.30.0",
    "eslint": "^8.18.0",
    "lint-staged": "^7.2.0",
    "prettier": "^2.7.1",
    "react": "16.4.2",
    "react-native": "^0.56.0",
    "semantic-release": "^17.0.4",
    "typescript": "3.8.3"
  },
  "keywords": [
    "android",
    "ios",
    "react-native",
    "speech",
    "voice"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "react-native": ">= 0.60.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/react-native-voice/voice.git"
  },
  "scripts": {
    "lint": "eslint src/*",
    "semantic-release": "semantic-release",
    "start": "yarn --cwd example start",
    "ios": "yarn --cwd example ios",
    "android": "yarn --cwd example android",
    "prepare": "npm run build && npm run build:plugin",
    "build": "tsc",
    "dev-sync": "cp -r ./dist example/node_modules/@react-native-voice/voice",
    "type-check": "tsc -noEmit",
    "build:plugin": "tsc --build plugin",
    "lint:plugin": "eslint plugin/src/*"
  },
  "dependencies": {
    "@expo/config-plugins": "^2.0.0",
    "invariant": "^2.2.4"
  }
}
